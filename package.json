{
    "name": "matcha",
    "version": "0.0.0",
    "scripts": {
        "start": "run-p start:client start:api",
        "start:client": "ng serve",
        "start:api": "tsx --watch server.ts",
        "build": "run-p build:client build:server",
        "build:client": "ng build",
        "build:server": "esbuild server.ts --bundle --platform=node --minify --outfile=dist/server.cjs",
        "build:server:meta": "esbuild server.ts --bundle --platform=node --minify --outfile=dist/server.cjs --metafile=dist/server.meta.json",
        "preview": "NODE_ENV=production node dist/server.cjs",
        "test": "ng test",
        "format": "prettier --write ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.1.0",
        "@angular/cdk": "^18.1.1",
        "@angular/common": "^18.1.0",
        "@angular/core": "^18.1.0",
        "@angular/forms": "^18.1.0",
        "@angular/material": "^18.1.1",
        "@angular/platform-browser": "^18.1.0",
        "@angular/platform-browser-dynamic": "^18.1.0",
        "@angular/router": "^18.1.0",
        "@rx-angular/template": "^18.0.2",
        "@tanstack/angular-query-devtools-experimental": "^5.55.4",
        "@tanstack/angular-query-experimental": "^5.55.4",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "ngxtension": "^4.0.0",
        "nodemailer": "^6.9.15",
        "pg": "^8.12.0",
        "postgres": "^3.4.4",
        "rxjs": "~7.8.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.1.1",
        "@angular/cli": "^18.1.1",
        "@angular/compiler": "^18.1.0",
        "@angular/compiler-cli": "^18.1.0",
        "@faker-js/faker": "^9.0.1",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.17",
        "@types/jasmine": "~5.1.0",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^18.18.0",
        "@types/nodemailer": "^6.4.15",
        "@types/pg": "^8.11.6",
        "autoprefixer": "^10.4.19",
        "esbuild": "^0.23.1",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "npm-run-all2": "^6.2.2",
        "postcss": "^8.4.40",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "tailwindcss": "^3.4.7",
        "tslib": "^2.3.0",
        "tsx": "^4.16.2",
        "typescript": "~5.5.2"
    }
}
